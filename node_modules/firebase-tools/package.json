{
  "name": "firebase-tools",
  "version": "11.14.3",
  "description": "Command-Line Interface for Firebase",
  "main": "./lib/index.js",
  "bin": {
    "firebase": "./lib/bin/firebase.js"
  },
  "files": [
    "lib",
    "schema",
    "standalone",
    "templates"
