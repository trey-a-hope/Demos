    "url": "https://github.com/firebase/firebase-tools/issues"
  },
  "homepage": "https://github.com/firebase/firebase-tools",
  "publishConfig": {
    "registry": "https://wombat-dressing-room.appspot.com"
  },
  "nyc": {
    "require": [
      "ts-node/register"
    ],
    "reporter": [
      "lcovonly",
      "text"
    ],
    "report-dir": "./.coverage",
    "extension": [
      ".js",
      ".ts"
    ],
    "exclude": [
      "src/test/**/*"
